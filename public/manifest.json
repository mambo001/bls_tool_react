{
  "key": "",
  "name": "BLS2 Tool - React",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Lets you add a scheduled scraper embedded in an app that persists data to browser.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"]
    }
  ],
  "browser_action": {
    "default_popup": "./popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://script.google.com/a/macros/google.com/s/AKfycbxivFmpPEs3So4NaC3Lk2BN5iE3ahxF4H4eTt8W-v64/"
  ],
  "web_accessible_resources": [
    "https://fonts.gstatic.com/s/materialicons/*"
  ]
}